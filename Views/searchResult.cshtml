<h1>Here is your search result</h1>
@if(Model.Count==0)
{
  <h3>Sorry! We cannot find artist you are looking for!</h3>
}
else
{
<ul>
  @foreach(var art in Model)
  {
    <li>Artist Name is : @art.GetArtist() </li>
    <li>Title : @art.GetTitle()</li>
  }
</ul>
}
<a href="/addNew"> Add More Cds</a>
<a href="/allCds"> View All Cds</a>
<a href="/searchByArtist"> Search Cds by Artists</a>
